{
  "name": "sanity-plugin-sitemeta",
  "version": "1.1.1",
  "description": "Collection of Sanity schematypes for Sanity Studio v3",
  "keywords": [
    "sanity",
    "schema",
    "typescript",
    "meta",
    "head",
    "helmet",
    "seo"
  ],
  "homepage": "https://github.com/AndreBClark/sanity-meta-schema#readme",
  "bugs": {
    "url": "https://github.com/AndreBClark/sanity-meta-schema/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AndreBClark/sanity-meta-schema.git"
  },
  "license": "BSD-3-Clause",
  "author": "Andre Clark <andre@cosmicdivision.dev>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^2.4.9",
    "@sanity/plugin-kit": "^3.1.10",
    "@types/node": "^18.17.17",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "commitlint": "^17.7.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.1",
    "sanity": "^3.16.7",
    "styled-components": "^5.3.11",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "stable": "*",
      "express": "1",
      "request": "*",
      "core-js": "*",
      "@hapi/*": "*",
      "popper-max-size-modifier": "*"
    }
  }
}
